year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{plantnet}: Automated Plant Identification with PlantNet",
         author = c(person("Tom", "August")),
         year = year,
         note = note,
         url = "https://github.com/BiologicalRecordsCentre/plantnet")
